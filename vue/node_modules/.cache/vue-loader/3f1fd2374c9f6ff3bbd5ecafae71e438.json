=\"newsdetail-container\">\r\n        <h1 class=\"title\">{{newsDetail.title}}</h1>\r\n        <p class=\"subtitle\">\r\n            <span>发表时间:{{newsDetail.add_time | datetimeFilter}}</span>\r\n            <span>点击:{{newsDetail.click}}次</span>\r\n        </p>\r\n        <div class=\"container\" v-html=\"newsDetail.content\"></div>\r\n        <comment-box></comment-box>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Toast from 'mint-ui';\r\n    import comment from '../subcompents/comment.vue';\r\n\r\n    export default{\r\n        data(){\r\n            return {\r\n                newsDetail:{},\r\n                nid:this.$route.params.nid\r\n            }\r\n        },\r\n        created() {\r\n            //console.log(this.$route.params.nid);\r\n            this.getnewsdetail();\r\n        },\r\n        methods:{\r\n            getnewsdetail(){\r\n                this.$http.get('newlist/detail/'+this.nid).then(result=>{\r\n                    //console.log(result);\r\n                    if(result.body.code==1){\r\n                        this.newsDetail=result.body.msg[0];\r\n                    }else{\r\n                        Toast(\"数据获取失败\");\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        components:{\r\n            'comment-box':comment\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .newsdetail-container{\r\n        padding: 10px 10px;\r\n    }\r\n    .newsdetail-container .title{\r\n        font-size:20px;\r\n        color:cadetblue;\r\n        text-align: center;\r\n    }\r\n    .newsdetail-container .subtitle{\r\n        font-size: 14px;\r\n        color:darkkhaki;\r\n        display: flex;\r\n        justify-content: space-between;\r\n    }\r\n    #img{\r\n        width: 375px;\r\n        height: 100px;\r\n    }\r\n</style>\r\n\r\n"]}]}